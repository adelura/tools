[alias]
	st = status
	co = checkout
	purr = pull --rebase
	fa = fetch --all
	hash = log -1 --format="%h"
	ri = rebase -i
	cam = commit --amend
	cad = commit --add
	camd = commit --add --amend
	
	develop = !sh -c 'git co develop && git fetch origin develop && git purr origin develop'
	
[core]
	editor = 'C:/Program Files (x86)/Notepad++/notepad++.exe' -multiInst -notabbar -nosession -noPlugin